														<object class="wxStaticText" name="valStr">
															<label>0.000</label>
															<wrap>-1</wrap>
														</object>
													</object>
													<object class="sizeritem">
														<option>0</option>
														<flag>wxALIGN_CENTER|wxLEFT</flag>
														<border>15</border>
														<object class="wxStaticText" name="lbBrushFocus">
															<label>Focus</label>
															<wrap>-1</wrap>
														</object>
													</object>
													<object class="sizeritem">
														<option>1</option>
														<flag>wxALL|wxEXPAND</flag>
														<border>5</border>
														<object class="wxSlider" name="brushFocus">
															<style>wxSL_BOTH|wxSL_HORIZONTAL</style>
															<size>-1,25</size>
															<value>0</value>
															<min>0</min>
															<max>1000</max>
														</object>
													</object>
													<object class="sizeritem">
														<option>0</option>
														<flag>wxALIGN_CENTER|wxRIGHT</flag>
														<border>15</border>
														<object class="wxStaticText" name="valFocus">
															<label>0.000</label>
															<wrap>-1</wrap>
