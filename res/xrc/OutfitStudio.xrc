<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">
	<object class="wxFrame" name="outfitStudio">
		<style>wxCAPTION|wxCLOSE_BOX|wxMAXIMIZE_BOX|wxMINIMIZE_BOX|wxRESIZE_BORDER|wxSYSTEM_MENU|wxTAB_TRAVERSAL</style>
		<size>990,757</size>
		<bg>#707070</bg>
		<title>Outfit Studio</title>
		<centered>1</centered>
		<aui_managed>0</aui_managed>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxEXPAND</flag>
				<border>0</border>
				<object class="wxToolBar" name="toolBarH">
					<style>wxTB_HORIZONTAL|wxTB_FLAT</style>
					<bg>#707070</bg>
					<fg>#ffffff</fg>
					<dontattachtoframe>1</dontattachtoframe>
					<object class="tool" name="btnNewProject">
						<label>New Project</label>
						<tooltip>Create a new project by selecting a reference body slider set, and outfit model files.</tooltip>
						<bitmap>$(AppDir)\res\images\NewProj.png</bitmap>
					</object>
					<object class="separator" />
					<object class="tool" name="btnLoadProject">
						<label>Load Project</label>
						<tooltip>Load a previously created slider set for editing.</tooltip>
						<bitmap>$(AppDir)\res\images\LoadProj.png</bitmap>
					</object>
					<object class="separator" />
					<object class="tool" name="btnSelect">
						<label>Select</label>
						<tooltip>Navigate and select meshes (or vertices in vertex mode).</tooltip>
						<bitmap>$(AppDir)\res\images\SelectBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\SelectBrush_d.png</bitmap2>
						<radio>1</radio>
						<checked>1</checked>
					</object>
					<object class="tool" name="btnMaskBrush">
						<label>Mask</label>
						<tooltip>Mask vertices to prevent them from being transformed.\nHold down the ALT key to remove masking.</tooltip>
						<bitmap>$(AppDir)\res\images\MaskBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\MaskBrush_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnInflateBrush">
						<label>Inflate</label>
						<tooltip>Increase mesh volume in an area.</tooltip>
						<bitmap>$(AppDir)\res\images\InflateBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\InflateBrush_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnDeflateBrush">
						<label>Deflate</label>
						<tooltip>Decrease mesh volume in an area.</tooltip>
						<bitmap>$(AppDir)\res\images\DeflateBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\DeflateBrush_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnMoveBrush">
						<label>Move</label>
						<tooltip>Move vertices over a plane parallel to the view.</tooltip>
						<bitmap>$(AppDir)\res\images\MoveBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\MoveBrush_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnSmoothBrush">
						<label>Smooth</label>
						<tooltip>Smooth an area of a mesh.</tooltip>
						<bitmap>$(AppDir)\res\images\SmoothBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\SmoothBrush_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnUndiffBrush">
						<label>Undiff</label>
						<tooltip>Undiff an area of a slider.</tooltip>
						<bitmap>$(AppDir)\res\images\UndiffBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\UndiffBrush_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnWeightBrush">
						<label>Weight Paint</label>
						<tooltip>Apply animation weight values for currently selected bone.\nHold down the ALT key to weaken the weighting.</tooltip>
						<bitmap>$(AppDir)\res\images\WeightBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\WeightBrush_d.png</bitmap2>
						<radio>1</radio>
						<disabled>1</disabled>
					</object>
					<object class="tool" name="btnColorBrush">
						<label>Color Paint</label>
						<tooltip>Apply vertex colors.\nHold down the ALT key to remove colors.</tooltip>
						<bitmap>$(AppDir)\res\images\ColorBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\ColorBrush_d.png</bitmap2>
						<radio>1</radio>
						<disabled>1</disabled>
					</object>
					<object class="tool" name="btnAlphaBrush">
						<label>Alpha Paint</label>
						<tooltip>Apply vertex alpha.\nHold down the ALT key to remove alpha.</tooltip>
						<bitmap>$(AppDir)\res\images\AlphaBrush.png</bitmap>
						<bitmap2>$(AppDir)\res\images\AlphaBrush_d.png</bitmap2>
						<radio>1</radio>
						<disabled>1</disabled>
					</object>
					<object class="tool" name="btnCollapseVertex">
						<label>Collapse Vertex</label>
						<tooltip>Deletes vertices with no more than three connections, without creating a hole.</tooltip>
						<bitmap>$(AppDir)\res\images\CollapseVertex.png</bitmap>
						<bitmap2>$(AppDir)\res\images\CollapseVertex_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnFlipEdgeTool">
						<label>Flip Edge</label>
						<tooltip>Flips mesh edges so that the opposite pair of vertices is connected.</tooltip>
						<bitmap>$(AppDir)\res\images\FlipEdge.png</bitmap>
						<bitmap2>$(AppDir)\res\images\FlipEdge_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="tool" name="btnSplitEdgeTool">
						<label>Split Edge</label>
						<tooltip>Splits a mesh edge in two with a new vertex.</tooltip>
						<bitmap>$(AppDir)\res\images\SplitEdge.png</bitmap>
						<bitmap2>$(AppDir)\res\images\SplitEdge_d.png</bitmap2>
						<radio>1</radio>
					</object>
					<object class="separator" />
					<object class="wxSlider" name="fovSlider">
						<size>100,25</size>
						<value>65</value>
						<min>20</min>
						<max>120</max>
						<tooltip>Field of View</tooltip>
					</object>
					<object class="wxStaticText" name="fovLabel">
						<fg>#ffffff</fg>
						<label>Field of View: 65</label>
					</object>
					<object class="space"/>
					<object class="tool" name="btnDiscord">
						<label>Discord</label>
						<tooltip>Open Discord invite link.</tooltip>
						<bitmap>$(AppDir)\res\images\Discord.png</bitmap>
					</object>
					<object class="tool" name="btnGitHub">
						<label>GitHub</label>
						<tooltip>Open GitHub link.</tooltip>
						<bitmap>$(AppDir)\res\images\GitHub.png</bitmap>
					</object>
					<object class="tool" name="btnPayPal">
						<label>PayPal</label>
						<tooltip>Open PayPal link.</tooltip>
						<bitmap>$(AppDir)\res\images\PayPal.png</bitmap>
					</object>
				</object>
			</object>
			<object class="sizeritem">
				<option>1</option>
				<flag>wxEXPAND</flag>
				<border>0</border>
				<object class="wxBoxSizer">
					<orient>wxHORIZONTAL</orient>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxEXPAND</flag>
						<border>0</border>
						<object class="wxToolBar" name="toolBarV">
							<style>wxTB_VERTICAL|wxTB_FLAT</style>
							<bg>#707070</bg>
							<fg>#ffffff</fg>
							<dontattachtoframe>1</dontattachtoframe>
							<object class="tool" name="btnTransform">
								<label>Transform</label>
								<tooltip>Shows a transform tool to manipulate shapes and vertices with.</tooltip>
								<bitmap>$(AppDir)\res\images\TransformTool.png</bitmap>
								<bitmap2>$(AppDir)\res\images\TransformTool_d.png</bitmap2>
								<toggle>1</toggle>
							</object>
							<object class="tool" name="btnPivot">
								<label>Pivot</label>
								<tooltip>Shows a pivot that can be moved and makes it the center of mesh operations like rotation and scale.</tooltip>
								<bitmap>$(AppDir)\res\images\Pivot.png</bitmap>
								<bitmap2>$(AppDir)\res\images\Pivot_d.png</bitmap2>
								<toggle>1</toggle>
							</object>
							<object class="tool" name="btnVertexEdit">
								<label>Vertex Edit</label>
								<tooltip>Lets you select vertices to add to or remove from the mask.\nClick on a vertex to select/unmask it.\nHold down CTRL to unselect/mask it.</tooltip>
								<bitmap>$(AppDir)\res\images\VertexEdit.png</bitmap>
								<bitmap2>$(AppDir)\res\images\VertexEdit_d.png</bitmap2>
								<toggle>1</toggle>
							</object>
							<object class="separator" />
							<object class="tool" name="btnXMirror">
								<label>X Mirror</label>
								<tooltip>Mirror edits across the X axis.</tooltip>
								<bitmap>$(AppDir)\res\images\XMirror.png</bitmap>
								<bitmap2>$(AppDir)\res\images\XMirror_d.png</bitmap2>
								<toggle>1</toggle>
								<checked>0</checked>
								<disabled>1</disabled>
							</object>
							<object class="tool" name="btnConnected">
								<label>Edit Connected Only\tC</label>
								<tooltip>Edit only vertices that are connected to the ones under the brush within the brush radius.</tooltip>
								<bitmap>$(AppDir)\res\images\EditConnected.png</bitmap>
								<toggle>1</toggle>
								<checked>0</checked>
							</object>
							<object class="tool" name="btnBrushCollision">
								<label>Global Brush Collision\tC</label>
								<tooltip>Allows for the brushes to collide with all currently selected meshes at the same time.</tooltip>
								<bitmap>$(AppDir)\res\images\BrushCollision.png</bitmap>
								<toggle>1</toggle>
								<checked>1</checked>
							</object>
							<object class="separator" />
							<object class="tool" name="btnViewFront">
								<label>View Front</label>
								<tooltip>Change camera view to the front.</tooltip>
								<bitmap>$(AppDir)\res\images\ViewFront.png</bitmap>
							</object>
							<object class="tool" name="btnViewBack">
								<label>View Back</label>
								<tooltip>Change camera view to the back.</tooltip>
								<bitmap>$(AppDir)\res\images\ViewBack.png</bitmap>
							</object>
							<object class="tool" name="btnViewLeft">
								<label>View Left</label>
								<tooltip>Change camera view to the left.</tooltip>
								<bitmap>$(AppDir)\res\images\ViewLeft.png</bitmap>
							</object>
							<object class="tool" name="btnViewRight">
								<label>View Right</label>
								<tooltip>Change camera view to the right.</tooltip>
								<bitmap>$(AppDir)\res\images\ViewRight.png</bitmap>
							</object>
							<object class="tool" name="btnViewPerspective">
								<label>Perspective View</label>
								<tooltip>Toggle perspective view.</tooltip>
								<bitmap>$(AppDir)\res\images\ViewPerspective.png</bitmap>
								<toggle>1</toggle>
								<checked>1</checked>
							</object>
							<object class="tool" name="btnShowNodes">
								<label>Show Nodes</label>
								<tooltip>Toggle rendering of nodes.</tooltip>
								<bitmap>$(AppDir)\res\images\Nodes.png</bitmap>
								<toggle>1</toggle>
							</object>
							<object class="tool" name="btnShowFloor">
								<label>Show Floor</label>
								<tooltip>Toggle rendering of the floor grid.</tooltip>
								<bitmap>$(AppDir)\res\images\Floor.png</bitmap>
								<toggle>1</toggle>
							</object>
						</object>
					</object>
					<object class="sizeritem">
						<option>1</option>
						<flag>wxEXPAND</flag>
						<border>0</border>
						<object class="wxSplitterWindow" name="splitter">
							<style>wxSP_3D|wxSP_LIVE_UPDATE</style>
							<sashpos>850</sashpos>
							<gravity>1</gravity>
							<minsize>250</minsize>
							<orientation>vertical</orientation>
